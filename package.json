{
  "name": "typenode",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "lint": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore",
    "lint:fix": "eslint . -c .eslintrc.js --ext .ts --ignore-path .eslintignore --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "nodemon",
    "serve": "ts-node ./src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm-cli": "ts-node node_modules/typeorm/cli.js --dataSource src/db/dataSource.ts",
    "db:sync": "ts-node node_modules/typeorm/cli.js migration:generate -p ./src/db/migrations/sync --dataSource ./src/db/dataSource.ts",
    "db:migrate:up": "ts-node node_modules/typeorm/cli.js migration:run --dataSource ./src/db/dataSource.ts",
    "db:migrate:down": "ts-node node_modules/typeorm/cli.js migration:revert --dataSource ./src/db/dataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.2",
    "sqlite3": "^5.0.8",
    "typeorm": "^0.3.7",
    "yup": "^0.32.11"
  }
}
